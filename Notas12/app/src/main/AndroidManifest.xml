<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gabriel.notas12">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo_2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_2_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.FiltrarActivity"></activity>
        <activity
            android:name=".activity.NovaNotaActivity"
            android:label="@string/nova_nota_activity"></activity>
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>